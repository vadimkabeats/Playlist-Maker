<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomToolbarTitle" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/title</item>
    </style>
    <style name="SettingsItemStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">@dimen/startPadding</item>
        <item name="android:paddingEnd">@dimen/endPadding</item>
        <item name="android:paddingVertical">@dimen/setPaddingVert</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/textSize</item>
    </style>
</resources>